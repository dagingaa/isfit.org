<!doctype html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="no"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="no"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="no"> <![endif]-->
<!-- Consider adding a manifest.appcache: h5bp.com/d/Offline -->
<!--[if gt IE 8]><!--> <html class="no-js" lang="no"> <!--<![endif]-->
<head>
  <title>ISFiT 2013</title>
  
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="ISFiT 2013 is the largest international student festival with a thematic focus in the world."/>
  <meta name="og:description" content="ISFiT 2013 is the largest international student festival with a thematic focus in the world."/>
  <meta name="viewport" content="width=device-width">
  <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />

  <script src="js/libs/modernizr-2.5.3.min.js"></script>

  <%= stylesheet_link_tag 'application' %>
  <%= javascript_tag "var AUTH_TOKEN = #{form_authenticity_token.inspect};" if protect_against_forgery? %>
  <%= csrf_meta_tag %>
  <%=yield :head%>
</head>


<body>


  <header class="span12"><!-- Start header -->

    <div class="navbar navbar-fixed-top"> <!-- Start tabs -->
      <div class="navbar-inner">
        <div class="container">
          <%= tabs %>

          <form class="navbar-form pull-left">
            <input type="search" class="search-query" placeholder="Search">
          </form>

        </div>
      </div>
    </div><!-- End tabs -->

    <div id="main-header-image">
      <%=link_to image_tag("isfit.png", :class=>"isfitlogo"), root_path %>
    </div>


  </header><!-- End header -->


  <div id="main" role="main" class="span12"> <!-- Start main -->
      

      <div id="venstreramme" class="span2"><!-- Start venstreramme -->
        <div class="boks">
          <h3 class="boxheadline">The ISFiT Way</h3>
           <%= link_to(image_tag('final.png') , 'http://www.isfit.org/assets/downloads/final.pdf') %>
        </div>

        <div class="boks">
          <h3 class="boxheadline">The Globe 2</h3>
          <%= link_to(image_tag('2nd_globe.jpg') , 'http://www.isfit.org/assets/downloads/theglobe_2nd.pdf') %>
        </div>
        
        <div class="boks">
          <h3 class="boxheadline">The Globe</h3>
          <%= link_to(image_tag('globe.jpg') , 'http://www.isfit.org/assets/downloads/globe.pdf') %>
        </div>
        
        <div id="boksbla_bg" class="alpha omega">
          <div id="boksbla" class="alpha omega">
          </div>
        </div>

      </div><!-- Slutt venstreramme -->
      


      <div id="midtramme" class="span6"><!-- Start midtramme -->
         <%
             if flash[:notice] %>
           <div id="notice">
             <%= flash[:notice] %>
           </div><%
             end

         if flash[:warnings] %>
       <div id="warning">
         Your application could not be sent because of the following conditions:
         <ul>
           <% flash[:warnings].each { |attr,msg|  %>
             <li><%= msg %></li>
           <% } %>
         </ul>
       </div> <%
         end

         if flash[:warning] %>
       <div id="warning">
         <%= flash[:warning] %>
       </div> <%
         end

         if flash[:warnings2] %>
       <div id="warning">
         Your application could not be sent because of the following conditions:
         <ul>
           <% flash[:warnings2].each { |attr,msg|  %>
             <li><%= attr + " " + msg %></li>
           <% } %>
         </ul>
       </div><%
         end %>
       <%= yield %>

     </div><!-- Slutt midtramme -->
     <div id="hoyreramme" class="span3"><!-- Start hoyreramme -->
      <div id="boksturkis_bg" class="alpha omega"><!-- Start turkis -->
         <div id="boksturkis" class="alpha omega">
           <div class="boks" onclick="location.href='<%=chronicle_path(params[:tab], Chronicle.find_most_recent.first)%>'" style="cursor:pointer">
             <h3 class="boxheadline">Kommentar</h3>

             <%= render :template =>'chronicles/index'%>
           </div>
         </div>
       </div><!-- Slutt turkis -->

       



      </div><!-- Slutt hoyreramme -->


  </div><!-- End main -->

  <footer class="span12"><!-- Start footer -->
    <%=link_to(image_tag("isfitliten.png", :class=>"footerlogo"), root_path)%>
    <p>
      Editor in Chief: Synne Mari Pedersen, editor[at]isfit.org. <br />
      All written material and the ISFiT logo is copyrighted the ISFiT Foundation © 2013.
    </p>
  </footer><!-- End footer -->



</script>


<!-- JavaScript at the bottom for fast page loading -->

  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="js/libs/jquery-1.7.1.min.js"><\/script>')</script>

  <!-- scripts concatenated and minified via build script -->
  <script src="js/plugins.js"></script>
  <script src="js/script.js"></script>
  <!-- end scripts -->

  <script type="text/javascript">
    var _gaq = _gaq || [];
    // _gaq.push(['_setAccount', 'UA-18016792-1']);
    // _gaq.push(['_setDomainName', 'isfit.org']);
    // _gaq.push(['_trackPageview']);
    // _gaq.push(['_trackLoadTime']);
    
    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
  </script>



</body>
<%= javascript_include_tag 'jquery','application', 'jquery.nivo.slider'%>
<%= yield :script %>
</html>