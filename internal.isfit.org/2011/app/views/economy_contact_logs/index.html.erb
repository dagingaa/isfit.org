<h1>Listing economy_contact_logs</h1>

<table>
  <tr>
    <th>Festival no</th>
    <th>Body</th>
    <th>Advice</th>
    <th>Author</th>
    <th>Unit</th>
  </tr>

<% for economy_contact_log in @economy_contact_logs %>
  <tr>
    <td><%=h economy_contact_log.festival_no %></td>
    <td><%=h economy_contact_log.body %></td>
    <td><%=h economy_contact_log.advice %></td>
    <td><%=h economy_contact_log.author %></td>
    <td><%#=h economy_contact_log.unit %></td>
    <td><%#= link_to 'Show', economy_contact_log %></td>
    <td><%#= link_to 'Edit', edit_economy_contact_log_path(economy_contact_log) %></td>
    <td><%#= link_to 'Destroy', economy_contact_log, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

