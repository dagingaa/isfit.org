<h1>Listing economy_contact_people</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Phone1</th>
    <th>Phone2</th>
    <th>Email</th>
    <th>Comment</th>
    <th>Unit</th>
  </tr>

<% for economy_contact_person in @economy_contact_people %>
  <tr>
    <td><%=h economy_contact_person.title %></td>
    <td><%=h economy_contact_person.firstname %></td>
    <td><%=h economy_contact_person.lastname %></td>
    <td><%=h economy_contact_person.phone1 %></td>
    <td><%=h economy_contact_person.phone2 %></td>
    <td><%=h economy_contact_person.email %></td>
    <td><%=h economy_contact_person.comment %></td>
    <td><%=h economy_contact_person.unit %></td>
    <td><%= link_to 'Show', economy_contact_person %></td>
    <td><%= link_to 'Edit', edit_economy_contact_person_path(economy_contact_person) %></td>
    <td><%= link_to 'Destroy', economy_contact_person, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

