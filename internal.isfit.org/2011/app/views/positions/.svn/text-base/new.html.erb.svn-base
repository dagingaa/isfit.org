<h1>Ny stilling for  </h1>
<%if @position.new_record?%>
	<%form_for @position , :url => { :action => "create"} do |f|%>
		<%=f.label(:title_no)%>
		<br>
		<%=f.text_field(:title_no, :class =>'text')%>
		<p>
		<%=f.label(:description_no)%>
		<br>
		<%=f.text_area(:description_no, :cols=>50, :rows=>10, :class=>'text')%>
		<p>
		<%=f.label(:title_en)%>
		<br>
		<%=f.text_field(:title_en, :class=>'text')%>
	  <p>
	  <%=f.label(:description_en)%>
		<br>
	 	<%=f.text_area(:description_en, :cols=>50, :rows=>10, :class=>'text')%>
		<p>
	 	<%=f.hidden_field(:section_id, :value=>params[:section_id])%>
	 	<%=f.submit('Lagre')%>
	<%end%>
<%else%>
	<%form_for @position , :url => { :action => "update"} do |f|%>
	<%=f.label(:title_no)%>
	<br>
	<%=f.text_field(:title_no, :class =>'text')%>
	<p>
	<%=f.label(:description_no)%>
	<br>
	<%=f.text_area(:description_no, :cols=>50, :rows=>10, :class=>'text')%>
	<p>
	<%=f.label(:title_en)%>
	<br>
	<%=f.text_field(:title_en, :class=>'text')%>
	<p>
	<%=f.label(:description_en)%>
	<br>
	<%=f.text_area(:description_en, :cols=>50, :rows=>10, :class=>'text')%>
	<p>
	<%=f.hidden_field(:section_id, :value=>params[:section_id])%>
	<%=f.submit('Lagre')%>
	<%end%>
<%end%>
