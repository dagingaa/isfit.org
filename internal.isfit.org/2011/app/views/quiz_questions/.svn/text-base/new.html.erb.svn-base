<h1>New quiz_question</h1>

<% form_for(@quiz_question) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :quiz %><br />
    <%= f.text_field :quiz %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', quiz_questions_path %>