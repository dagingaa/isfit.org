<table>
	<tr>
		<td width="150"><b>ID</b></td>
		<td><%= @wop_proposition.id %></td>
	</tr>
	<tr>
		<td><b>Name</b></td>
		<td><%= @wop_proposition.lastname + ', ' + @wop_proposition.firstname %></td>
	</tr>
	<tr>
		<td><b>E-mail</b></td>
		<td><%= @wop_proposition.email %></td>
	</tr>
	<tr>
		<td><b>Phone</b></td>
		<td><%= @wop_proposition.phone %></td>
  </tr>
  <tr>
		<td><b>Address</b></td>
		<td><%= @wop_proposition.address %></td>
  </tr>
  <tr>
		<td><b>Zip-code</b></td>
		<td><%= @wop_proposition.zipcode %></td>
  </tr>
  <tr>
		<td><b>Study place</b></td>
		<td><%= @wop_proposition.study_place %></td>
  </tr>
  <tr>
    <td><b>Description</b></td>
    <td><%=@wop_proposition.description%></td>
  </tr>

</table>



<%= link_to 'Back', wop_propositions_path(params[:tab]) %>
